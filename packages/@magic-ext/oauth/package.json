  },
  "externals": {
    "include": [
      "@magic-sdk/commons",
      "@magic-sdk/provider"
    ]
  },
  "dependencies": {
    "crypto-js": "^3.3.0"
  },
  "devDependencies": {
    "@magic-sdk/commons": "^24.18.0",
    "@types/crypto-js": "~3.1.47"
